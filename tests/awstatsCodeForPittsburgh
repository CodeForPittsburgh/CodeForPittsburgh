cd /usr/local/awstats/wwwroot/cgi-bin
sudo perl awstats.pl -update -config=CodeForPittsburgh
sudo perl awstats.pl -config=CodeForPittsburgh -output -staticlinks > /var/www/html/CodeForPittsburgh/awstats.CodeForPittsburgh.html
